<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="oner">
    <select id="findMyRestaurant" resultType="restaurant">
        select
            *
        from
            restaurant
        where
            users_no = #{userno}
    </select>
</mapper>